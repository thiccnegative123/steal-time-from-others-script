local function runLoop()
	while task.wait() do
		if lol == 0 then
			local character = player.Character or player.CharacterAdded:Wait()
			local humanoidroot = character:FindFirstChild("HumanoidRootPart")
			if not humanoidroot then continue end
			local nearestPlayer = nil
			local shortestDistance = math.huge
			for _, otherPlayer in ipairs(game.Players:GetPlayers()) do
				if otherPlayer ~= player and otherPlayer.Character and otherPlayer.Character:FindFirstChild("HumanoidRootPart") then
					local otherHRP = otherPlayer.Character.HumanoidRootPart
					local distance = (otherHRP.Position - humanoidroot.Position).Magnitude
					if distance < shortestDistance then
						shortestDistance = distance
						nearestPlayer = otherPlayer
					end
				end
			end
			if nearestPlayer and shortestDistance <= 15 then
				local targetHRP = nearestPlayer.Character.HumanoidRootPart
				local behindCFrame = targetHRP.CFrame * CFrame.new(0, 0, 6)
				humanoidroot.CFrame = behindCFrame
				print("Teleported behind: " .. nearestPlayer.Name)
			end
		end
	end
end

local function runScript()
	game.StarterGui:SetCore("SendNotification", {
		Title = "BEST STFO SCRIPT";
		Text = "Made by Haxarebest1";
		Duration = 5
	})
	game.StarterGui:SetCore("SendNotification", {
		Title = "BEST STFO SCRIPT";
		Text = "and ChatGPT";
		Duration = 5
	})
	task.spawn(runLoop)
end

credit.MouseButton1Click:Connect(function()
	frame1.Visible = not frame1.Visible
end)

button.MouseButton1Click:Connect(function()
	if lol == 1 then
		lol = 0
		button.Text = "tp kill OP(on)"
	else
		lol = 1
		button.Text = "tp kill OP(off)"
	end
end)

player.CharacterAdded:Connect(function()
	task.wait()
end)

runScript()

button1.MouseButton1Click:Connect(function()
local sword = game:GetService("Players").LocalPlayer.Backpack:FindFirstChild("Sword") or game:GetService("Players").LocalPlayer.Backpack:FindFirstChild("ClassicSword") or
game:GetService("Players").LocalPlayer.Backpack:FindFirstChild("Classic Sword") or
game:GetService("Players").LocalPlayer.Backpack:FindFirstChild("ChristmasSword") or
game:GetService("Players").LocalPlayer.Backpack:FindFirstChild("Foil") or
game:GetService("Players").LocalPlayer.Backpack:FindFirstChild("Linked Sword") or
game:GetService("Players").LocalPlayer.Backpack:FindFirstChild("Black Sword") or
game:GetService("Players").LocalPlayer.Backpack:FindFirstChild("Thunder Sword") or
game:GetService("Players").LocalPlayer.Backpack:FindFirstChild("Classic") or
game:GetService("Players").LocalPlayer.Backpack:FindFirstChild("Sharpened") or
game:GetService("Players").LocalPlayer.Backpack:FindFirstChild("Medical") or
game:GetService("Players").LocalPlayer.Backpack:FindFirstChild("Linked") or
game:GetService("Players").LocalPlayer.Backpack:FindFirstChild("Default") or
game:GetService("Players").LocalPlayer.Backpack:FindFirstChild("ClassicSwordV2")
if sword then
    sword.Grip = CFrame.new(2, 1.25, 0, 0.0995037332, 0.995037258, 0, 0.995037258, -0.0995035842, 0, 0, 0, -1)
else
    warn("Sword not found in Backpack!")
end
end)

button2.MouseButton1Click:Connect(function()
	local character = player.Character or player.CharacterAdded:Wait()
	local humanoid = character:WaitForChild("Humanoid")

	if lol2 == 1 then
		local anim = Instance.new("Animation")
		anim.AnimationId = "rbxassetid://282574440"
		carpettrack = humanoid:LoadAnimation(anim)
		carpettrack:Play()
		button2.Text = "carpet(on)"
		lol2 = 0
	else
		if carpettrack then
			carpettrack:Stop()
		end
		button2.Text = "carpet(off)"
		lol2 = 1
	end
end)

button3.MouseButton1Click:Connect(function()
loadstring(game:HttpGet("https://raw.githubusercontent.com/Brew-Softworks/BrewReach/refs/heads/main/loader.lua"))()
end)

button4.MouseButton1Click:Connect(function()
local humanoid = game.Players.LocalPlayer.Character.Humanoid
if lol0 == 5 then
humanoid.HipHeight = 7
button4.Text = "HipHeight = 5"
lol0 = 1
else
humanoid.HipHeight = 0
button4.Text = "HipHeight = 0"
lol0 = 5
end
end)

button5.MouseButton1Click:Connect(function() loadstring(game:HttpGet('https://pastebin.com/raw/3Rnd9rHf'))()
end)

